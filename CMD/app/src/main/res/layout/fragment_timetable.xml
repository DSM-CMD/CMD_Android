<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#232323"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Fragment.TimetableFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint"
        android:layout_width="match_parent"
        android:layout_height="168dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">

        <TextView
            android:id="@+id/tvtimetable"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="시간표"
            android:textSize="50sp"
            android:textColor="@color/white"
            android:fontFamily="@font/notosans"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="32dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"/>

        <TextView
            android:id="@+id/tvdate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/white"
            android:layout_marginBottom="8dp"
            app:layout_constraintLeft_toLeftOf="@id/tvtimetable"
            app:layout_constraintBottom_toBottomOf="parent"/>


    </androidx.constraintlayout.widget.ConstraintLayout>

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="168dp"
        app:layout_constraintTop_toBottomOf="@+id/constraint"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="314dp"
            android:layout_height="64dp"
            android:layout_gravity="center_horizontal">

            <androidx.cardview.widget.CardView
                android:id="@+id/cvperiod01"
                android:layout_width="304dp"
                android:layout_height="64dp"
                android:layout_marginTop="64dp"
                android:backgroundTint="@color/gray"
                app:cardCornerRadius="12dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/tvperiod01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="25sp"
                    android:textColor="#232323" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvperiod02"
                android:layout_width="304dp"
                android:layout_height="64dp"
                android:layout_marginTop="20dp"
                android:backgroundTint="@color/gray"
                app:cardCornerRadius="12dp"
                app:layout_constraintTop_toBottomOf="@id/cvperiod01"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent">

                <TextView
                    android:id="@+id/tvperiod02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="25sp"
                    android:textColor="#232323" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvperiod03"
                android:layout_width="304dp"
                android:layout_height="64dp"
                android:layout_marginTop="20dp"
                android:backgroundTint="@color/gray"
                app:cardCornerRadius="12dp"
                app:layout_constraintTop_toBottomOf="@id/cvperiod02"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent">

                <TextView
                    android:id="@+id/tvperiod03"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="25sp"
                    android:textColor="#232323" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvperiod04"
                android:layout_width="304dp"
                android:layout_height="64dp"
                android:layout_marginTop="20dp"
                android:backgroundTint="@color/gray"
                app:cardCornerRadius="12dp"
                app:layout_constraintTop_toBottomOf="@id/cvperiod03"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent">

                <TextView
                    android:id="@+id/tvperiod04"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="25sp"
                    android:textColor="#232323" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvperiod05"
                android:layout_width="304dp"
                android:layout_height="64dp"
                android:layout_marginTop="20dp"
                android:backgroundTint="@color/gray"
                app:cardCornerRadius="12dp"
                app:layout_constraintTop_toBottomOf="@id/cvperiod04"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent">

                <TextView
                    android:id="@+id/tvperiod05"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="25sp"
                    android:textColor="#232323" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvperiod06"
                android:layout_width="304dp"
                android:layout_height="64dp"
                android:layout_marginTop="20dp"
                android:backgroundTint="@color/gray"
                app:cardCornerRadius="12dp"
                app:layout_constraintTop_toBottomOf="@id/cvperiod05"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent">

                <TextView
                    android:id="@+id/tvperiod06"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="25sp"
                    android:textColor="#232323" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvperiod07"
                android:layout_width="304dp"
                android:layout_height="64dp"
                android:layout_marginTop="20dp"
                android:backgroundTint="@color/gray"
                app:cardCornerRadius="12dp"
                app:layout_constraintTop_toBottomOf="@id/cvperiod06"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent">

                <TextView
                    android:id="@+id/tvperiod07"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="25sp"
                    android:textColor="#232323" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvperiod08"
                android:layout_width="304dp"
                android:layout_height="64dp"
                android:layout_marginTop="20dp"
                android:backgroundTint="@color/gray"
                app:cardCornerRadius="12dp"
                app:layout_constraintTop_toBottomOf="@id/cvperiod07"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent">

                <TextView
                    android:id="@+id/tvperiod08"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="25sp"
                    android:textColor="#232323" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvperiod09"
                android:layout_width="304dp"
                android:layout_height="64dp"
                android:layout_marginTop="20dp"
                android:backgroundTint="@color/gray"
                app:cardCornerRadius="12dp"
                app:layout_constraintTop_toBottomOf="@id/cvperiod08"
                app:layout_constraintLeft_toLeftOf="parent"
